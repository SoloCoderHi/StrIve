{
  "firestore": {
    "rules": "firestore.rules"
  },
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      { "source": "/lists/*/import/analyze", "function": "analyzeListImport" },
      { "source": "/lists/*/import/confirm", "function": "confirmListImport" },
      { "source": "/lists/**", "function": "listsExport" }
    ]
  },
  "functions": {
    "source": "functions"
  }
}


